{
  "name": "eslint-plugin-eslint-performance",
  "version": "0.1.0",
  "description": "ESLint plugin to detect performance anti-patterns and suggest optimizations",
  "type": "module",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "performance",
    "optimization",
    "reduce",
    "spread",
    "array"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "files": [
    "lib",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JonasBa/eslint-performance.git"
  },
  "bugs": {
    "url": "https://github.com/JonasBa/eslint-performance/issues"
  },
  "homepage": "https://github.com/JonasBa/eslint-performance#readme",
  "author": "JonasBa <jonas.badalic@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">=7.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.37.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.11.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
